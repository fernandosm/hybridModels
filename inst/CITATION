year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{hybridModels}: R package to simulate stochastic hybrid models for transmssion of diseases in dynamic networks",
         author = c(person("Marques", "Fernando S"),
                    person("Amaku", "Marcos"),
                    person("Grisi Filho", "Jose H H")),
         year = year,
         note = note,
         url = "https://github.com/fernandosm/hybridModels")